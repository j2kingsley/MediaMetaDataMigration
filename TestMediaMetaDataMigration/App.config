<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
      <!-- log4net Config-->
      <add key="log4net.Config" value="log4net.config" />
      <add key="log4net.Config.Watch" value="True" />

      <!-- Other Configs-->
      <add key="refreshInterval" value="10" />
      <!--  In Seconds-->

      <!--Postgress Config-->
      <add key="dbHost" value="localhost" />
      <add key="dbPort" value="5432" />
      <add key="dbName" value="PropertyDevDb" />
      <add key="dbUserName" value="postgres" />
      <add key="dbPassword" value="password" />

    
    </appSettings>

</configuration>